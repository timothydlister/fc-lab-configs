config antivirus quarantine
    set agelimit 0
    set maxfilesize 0
    set quarantine-quota 0
    unset drop-infected
    set store-infected imap smtp pop3 http ftp nntp imaps smtps pop3s https ftps mapi cifs
    unset drop-blocked
    set store-blocked imap smtp pop3 http ftp nntp imaps smtps pop3s ftps mapi cifs
    unset drop-heuristic
    set store-heuristic imap smtp pop3 http ftp nntp imaps smtps pop3s https ftps mapi cifs
    set lowspace ovrw-old
    set destination disk
end
config antivirus profile
    edit "default"
        set comment "Scan files and block viruses."
        set replacemsg-group ''
        set inspection-mode flow-based
        set mobile-malware-db enable
        config http
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config ftp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config imap
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config pop3
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config smtp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config nntp
            unset options
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config smb
            unset options
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config nac-quar
            set infected none
            set log disable
        end
        set av-virus-log enable
        set av-block-log enable
        set extended-log disable
        set scan-mode full
    next
